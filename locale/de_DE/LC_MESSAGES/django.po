msgstr "Vorname"

#: mafiasi/registration/templates/registration/require_info.html:22
msgid "Last name"
msgstr "Nachname"

#: mafiasi/registration/templates/registration/require_info.html:26
msgid "Year of study start"
msgstr "Jahrgang"

#: mafiasi/registration/templates/registration/token_expired.html:5
msgid "Token expired"
msgstr "Token abgelaufen"

#: mafiasi/registration/templates/registration/token_expired.html:6
msgid "Sorry, but the provided token is expired."
msgstr "Tut uns leid, aber der angegebene Token ist abgelaufen."

#: mafiasi/registration/templates/registration/token_invalid.html:5
msgid "Invalid token"
msgstr "Ungültiger Token"

#: mafiasi/registration/templates/registration/token_invalid.html:6
msgid "Sorry, but the provided token is invalid."
msgstr "Tut uns leid, aber der angegebene Token ist ungültig."

